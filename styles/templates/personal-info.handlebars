<h1>{{names.first}} {{names.last}}</h1>
<img class="photo" src="{{photoUrl}}" alt="photo of me">
<ul class="contacts">
    {{#each contacts}}
        <li>
            <img class="img-contact" src="{{iconUrl}}">
            {{#ifEqual type "email"}}
                <a href="mailto:{{content}}">{{this.content}}</a>
            {{else}}
                <em>{{content}}</em>
            {{/ifEqual}}
        </li>
    {{/each}}
</ul>

<h3>About me</h3>
<p>{{aboutMe}}</p>